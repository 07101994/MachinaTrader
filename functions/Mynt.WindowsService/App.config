<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/>
    </startup>
  <appSettings file="config/appSettingsOverwrites.config">
    <add key="OverwritingExample" value="Retrieved from App.config"/>
    <add key="TradeJobCronTrigger" value="10 0/5 * * * ?"/>
    <add key="RequestDebuggerOnStartup" value="false"/>    
    <!-- Set true if log4net misbehaves... Or PEBCAK -->
    <add key="log4net.Internal.Debug" value="false"/>

  </appSettings>
</configuration>
